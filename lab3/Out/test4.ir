FUNCTION add :
PARAM temp
t1 := *temp
t0 := temp + #4
t2 := *t0
t3 := t1 + t2
RETURN t3
FUNCTION main :
DEC op_var 8
op := &op_var
DEC r_var 8
r := &r_var
i := #0
j := #0
LABEL label1 :
IF i >= #2 GOTO label0
LABEL label3 :
IF j >= #2 GOTO label2
t4 := j * #4
t5 := op + t4
t6 := i + j
*t5 := t6
t7 := j + #1
j := t7
GOTO label3
LABEL label2 :
t8 := i * #4
t9 := r + t8
ARG op
t10 := CALL add
*t9 := t10
t11 := i * #4
t12 := r + t11
t13 := *t12
WRITE t13
t14 := i + #1
i := t14
j := #0
GOTO label1
LABEL label0 :
RETURN #0
