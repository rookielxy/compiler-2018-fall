FUNCTION add :
PARAM temp
t0 := #0
t1 := temp + t0
t4 := *t1
t2 := #4
t3 := temp + t2
t5 := *t3
t6 := t4 + t5
RETURN t6
FUNCTION main :
DEC op_var 8
op := &op_var
DEC r_var 8
r := &r_var
i := #0
j := #0
LABEL label1 :
IF i >= #2 GOTO label0
LABEL label3 :
IF j >= #2 GOTO label2
t7 := j * #4
t8 := op + t7
t9 := i + j
*t8 := t9
t10 := j + #1
j := t10
GOTO label3
LABEL label2 :
t11 := #0
t12 := r + t11
t13 := i * #4
t14 := t12 + t13
ARG op
t15 := CALL add
*t14 := t15
t16 := #0
t17 := r + t16
t18 := i * #4
t19 := t17 + t18
t20 := *t19
WRITE t20
t21 := i + #1
i := t21
j := #0
GOTO label1
LABEL label0 :
RETURN #0
