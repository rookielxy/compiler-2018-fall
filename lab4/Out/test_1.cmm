.data
_prompt: .asciiz "Entering an integer:"_ret: .asciiz "
".globl main.textread:
	li $v0, 4
	la $a0, _prompt
	syscall
	li $v0, 5
	syscall
	jr $ra
write:
	li $v0, 1
	syscall
	li $v0, 4
	la $a0, ret
	syscall
	move $v0, $0
	jr $ra
main:
	addi $sp,  $sp, -4
	sw $fp, 0($sp)
	move $fp, $sp
	addi $sp,  $sp, -4
	li $t0, 0
